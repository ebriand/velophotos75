{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "globals": {
    "fetch": false
  },
  "rules": {
    "semi": [2, "never"],
    "no-console": 0,
    "arrow-body-style": 0,
    "import/no-named-as-default": 0,
    "import/prefer-default-export": 0,
    "import/no-unresolved": 0,
    "react/forbid-prop-types": 0,
    "import/no-extraneous-dependencies":
      [ "error", { "devDependencies": true } ]
  }
}
