{
  "name": "velophotos75",
  "version": "1.0.0",
  "description": "Galerie de photos de courses cyclistes",
  "main": "server/server.js",
  "repository": "git@github.com:ebriand/velophotos75.git",
  "author": "Eric BRIAND",
  "license": "MIT",
  "scripts": {
    "test": "cross-env NODE_ENV=test echo \"Error: no test specified\" && exit 1",
    "start": "concurrently \"yarn run start:back\" \"yarn run start:front\"",
    "start:back": "cross-env COMPONENT=back nodemon --watch \"src/server/**/*.js\" --exec \"webpack --colors --debug && node --debug dist/server/server.js\"",
    "start:front": "webpack-dev-server",
    "build": "cross-env NODE_ENV=production webpack --progress --display-error-details",
    "clean": "rimraf dist",
    "prestart": "yarn run clean",
    "prebuild": "yarn run clean"
  },
  "devDependencies": {
    "babel-core": "6.20.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.9",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "concurrently": "3.1.0",
    "cross-env": "3.1.3",
    "eslint": "3.12.0",
    "eslint-config-airbnb": "13.0.0",
    "eslint-loader": "1.6.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.8.0",
    "file-url": "2.0.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "2.24.1",
    "nodemon": "1.11.0",
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "rimraf": "2.5.4",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2",
    "webpack-node-externals": "1.5.4"
  },
  "dependencies": {
    "bluebird": "3.4.6",
    "boom": "4.2.0",
    "good": "7.1.0",
    "good-console": "6.2.0",
    "good-squeeze": "5.0.1",
    "hapi": "16.0.1",
    "inert": "4.0.3",
    "lodash": "4.17.2",
    "request": "2.79.0",
    "request-promise": "4.1.1",
    "source-map-support": "0.4.6"
  }
}
